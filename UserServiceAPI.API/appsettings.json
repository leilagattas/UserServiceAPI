{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=${DB_SCHEMA};User=${DB_USER};Password=${DB_PASSWORD};"
  },
  "Jwt": {
    "Key": "${JWT_KEY}",
    "Issuer": "https://localhost:44383/",
    "Audience": "https://localhost:44383/"
  }
}
